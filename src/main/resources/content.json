      "pic_name": "1541140737.jpg",
      "pic_part": [
        {
          "id": "91617",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.074",
          "left": "0.083",
          "width": "0.175",
          "height": "0.052",
          "chinese": "语法聚焦",
          "english": "Grammar Focus",
          "spoken_type": "2",
          "audio": "1541574912_038.mp3",
          "order": "1",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/f0125ed49fe8b85dda537616f5cec59f/upload/audio/1541574912_038.mp3"
        },
        {
          "id": "91618",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.138",
          "left": "0.116",
          "width": "0.237",
          "height": "0.03",
          "chinese": "怎么了？",
          "english": "What's the matter?",
          "spoken_type": "2",
          "audio": "1541574912_14.mp3",
          "order": "2",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/00da277958281877b09c92a97790c09b/upload/audio/1541574912_14.mp3"
        },
        {
          "id": "91619",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.133",
          "left": "0.425",
          "width": "0.255",
          "height": "0.034",
          "chinese": "我胃痛。",
          "english": "I have a stomachache.",
          "spoken_type": "2",
          "audio": "1541574912_220.mp3",
          "order": "3",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/3d13ece8f59a09a45441130b73a8613e/upload/audio/1541574912_220.mp3"
        },
        {
          "id": "91620",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.139",
          "left": "0.691",
          "width": "0.257",
          "height": "0.039",
          "chinese": "你下次不应该吃这么多。",
          "english": "You shouldn't eat so much next time.",
          "spoken_type": "2",
          "audio": "1541574912_336.mp3",
          "order": "4",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/880e125f68f1818aca6a64c105db4898/upload/audio/1541574912_336.mp3"
        },
        {
          "id": "91621",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.176",
          "left": "0.119",
          "width": "0.316",
          "height": "0.031",
          "chinese": "本怎么了？",
          "english": "What's the matter with Ben?",
          "spoken_type": "2",
          "audio": "1541574912_471.mp3",
          "order": "5",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/3b3221dfdd66532c4039a51cf48e7fd2/upload/audio/1541574912_471.mp3"
        },
        {
          "id": "91622",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.176",
          "left": "0.439",
          "width": "0.252",
          "height": "0.044",
          "chinese": "他伤了自己。他背痛。",
          "english": "He hurt himself.He has a sore back.",
          "spoken_type": "2",
          "audio": "1541574912_525.mp3",
          "order": "6",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/25f1e7b9d3cb247d16b2a0763a8ec2e2/upload/audio/1541574912_525.mp3"
        },
        {
          "id": "91623",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.176",
          "left": "0.695",
          "width": "0.247",
          "height": "0.044",
          "chinese": "他应该躺下来休息。",
          "english": "He should lie down and rest.",
          "spoken_type": "2",
          "audio": "1541574912_62.mp3",
          "order": "7",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/ae6dfd0131f000b4e5b4c674575393e6/upload/audio/1541574912_62.mp3"
        },
        {
          "id": "91624",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.219",
          "left": "0.125",
          "width": "0.32",
          "height": "0.022",
          "chinese": "你发烧了吗？",
          "english": "Do you have a fever?",
          "spoken_type": "2",
          "audio": "1541574912_780.mp3",
          "order": "8",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/558d006e498550cc918519153a658268/upload/audio/1541574912_780.mp3"
        },
        {
          "id": "91625",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.215",
          "left": "0.445",
          "width": "0.332",
          "height": "0.028",
          "chinese": "是的,我发烧了。/不,我没有发烧。/我不知道。",
          "english": "Yes, I do./No, I don't./I don't know.",
          "spoken_type": "2",
          "audio": "1541574912_856.mp3",
          "order": "9",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/9e930114dc10bc664b20cc9901bde57e/upload/audio/1541574912_856.mp3"
        },
        {
          "id": "91626",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.243",
          "left": "0.115",
          "width": "0.321",
          "height": "0.036",
          "chinese": "他牙痛吗？",
          "english": "Does he have a toothache?",
          "spoken_type": "2",
          "audio": "1541574912_926.mp3",
          "order": "10",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/809ca05a84608fb18ff39b7f88b05d09/upload/audio/1541574912_926.mp3"
        },
        {
          "id": "91627",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.242",
          "left": "0.445",
          "width": "0.175",
          "height": "0.038",
          "chinese": "是的，他牙痛。",
          "english": "Yes, he does.",
          "spoken_type": "2",
          "audio": "1541574912_1048.mp3",
          "order": "11",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/19e7336ad0561a2e16564354f0d1fbcf/upload/audio/1541574912_1048.mp3"
        },
        {
          "id": "91628",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.244",
          "left": "0.692",
          "width": "0.265",
          "height": "0.042",
          "chinese": "他应该去看牙医并照X光。",
          "english": "He should see a dentist and get an X-ray.",
          "spoken_type": "2",
          "audio": "1541574912_1115.mp3",
          "order": "12",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/d9afb83409102a3d2e040b6bcad640fe/upload/audio/1541574912_1115.mp3"
        },
        {
          "id": "91629",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.28",
          "left": "0.115",
          "width": "0.256",
          "height": "0.029",
          "chinese": "她应该做什么？",
          "english": "What should she do?",
          "spoken_type": "2",
          "audio": "1541574912_1239.mp3",
          "order": "13",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/a582be69eec1ac71a827db69ce20e834/upload/audio/1541574912_1239.mp3"
        },
        {
          "id": "91630",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.286",
          "left": "0.521",
          "width": "0.365",
          "height": "0.023",
          "chinese": "她应该量一量她的体温。",
          "english": "She should take her temperature.",
          "spoken_type": "2",
          "audio": "1541574912_1312.mp3",
          "order": "14",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/3c9cab81b5a9abdc398ddd1e4b4a3292/upload/audio/1541574912_1312.mp3"
        },
        {
          "id": "91631",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.311",
          "left": "0.116",
          "width": "0.383",
          "height": "0.023",
          "chinese": "我应该在上面敷些药吗？",
          "english": "Should I put some medicine on it?",
          "spoken_type": "2",
          "audio": "1541574912_1464.mp3",
          "order": "15",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/d165228c590ef5e22d37025eac2ecbe4/upload/audio/1541574912_1464.mp3"
        },
        {
          "id": "91632",
          "create_time": "2018-11-07 15:15:12",
          "pid": "42047",
          "top": "0.309",
          "left": "0.531",
          "width": "0.388",
          "height": "0.029",
          "chinese": "是的,你应该。/不,你不应该。",
          "english": "Yes, you should./No, you shouldn't.

          ",
          "spoken_type": "2",
          "audio": "1541574912_1563.mp3",
          "order": "16",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/c4c9a83b97f5bee563c387cbdf9f07ff/upload/audio/1541574912_1563.mp3"
        },
        {
          "id": "91633",
          "create_time": "2018-11-07 15:19:22",
          "pid": "42047",
          "top": "0.771",
          "left": "0.6",
          "width": "0.359",
          "height": "0.044",
          "chinese": "怎么了？你踢足球的时候伤到自己了吗？",
          "english": "What's the matter? Did you hurt yourself playing soccer?",
          "spoken_type": "2",
          "audio": "1541575162_074.mp3",
          "order": "17",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/7f5c1d23e117993a105480ce798a5cdb/upload/audio/1541575162_074.mp3"
        },
        {
          "id": "91634",
          "create_time": "2018-11-07 15:19:23",
          "pid": "42047",
          "top": "0.813",
          "left": "0.579",
          "width": "0.231",
          "height": "0.022",
          "chinese": "不,我没有。",
          "english": "No, I didn't.",
          "spoken_type": "2",
          "audio": "1541575162_1100.mp3",
          "order": "18",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/b917998059f7b4958b0548c210c45896/upload/audio/1541575162_1100.mp3"
        },
        {
          "id": "91635",
          "create_time": "2018-11-07 15:19:23",
          "pid": "42047",
          "top": "0.838",
          "left": "0.605",
          "width": "0.256",
          "height": "0.019",
          "chinese": "你跌倒了吗？",
          "english": "Did you fall down?",
          "spoken_type": "2",
          "audio": "1541575163_219.mp3",
          "order": "19",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/353585306822e8b3635c9e0e1a794e88/upload/audio/1541575163_219.mp3"
        },
        {
          "id": "91636",
          "create_time": "2018-11-07 15:19:23",
          "pid": "42047",
          "top": "0.856",
          "left": "0.599",
          "width": "0.177",
          "height": "0.022",
          "chinese": "是的。",
          "english": "Yes, I did.",
          "spoken_type": "2",
          "audio": "1541575163_313.mp3",
          "order": "20",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/bab176d08571765f702d283f4787e8c7/upload/audio/1541575163_313.mp3"
        },
        {
          "id": "91637",
          "create_time": "2018-11-07 15:19:23",
          "pid": "42047",
          "top": "0.879",
          "left": "0.596",
          "width": "0.372",
          "height": "0.04",
          "chinese": "你应该回家休息。",
          "english": "You should go home and get some rest.",
          "spoken_type": "2",
          "audio": "1541575163_415.mp3",
          "order": "21",
          "audio2": "",
          "audiooss": "http://cdn.dianducdn.beisu100.com/202002041318/1c8818174c1df96d84306108f6fc1bcb/upload/audio/1541575163_415.mp3"
        }
      ]
